<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="project_project_form_inherit" model="ir.ui.view">
        <field name="name">project.project.form.inherit</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">

            <xpath expr="//group[@name='misc']" position="after">
                <group name="rounding">
                    <group string="Time rounding" name="time_rounding_parameters">
                        <field name="timesheet_rounding_granularity"/>
                        <field name="timesheet_rounding_method"/>
                        <field name="timesheet_invoicing_factor"/>
                    </group>
                </group>
            </xpath>

        </field>
    </record>
</odoo>
